<h2> QUEST<%= @quest.id %> </h2>

<!-- google maps api div -->

<div class="map-wrapper">
  <div id="map"></div>
</div>

<div class="task">
  <p>
    Sticker located under bench
  </p>
  <form action="/show" method="post">
    <label for="">Enter your code: </label>
    <input type="text" value="Your answer">
  <button>Submit</button>
</form>

</div>

<!-- google maps api script -->

<script>
  function initMap() {

    var taskLocation = {lat: -37.8182711, lng: 144.9670618} 
    // var taskLocation = {lat: <%#= @quest.lat%> , lng: <%#=@quest.long %>}

    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 16,
      center: taskLocation
    })
    var marker1 = new google.maps.Marker({
      position: taskLocation,
      map: map
    })
  }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAT54WwO2XSLwmeaRvNjQdamDP6pma-HQM&callback=initMap" async defer>
</script>




